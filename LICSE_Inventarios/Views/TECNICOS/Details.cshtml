@model LICSE_Inventarios.Models.TECNICO


    ViewBag.Title = "Details";
}


<!-- top bar -->
<div class="top-bar solicitudes">
    <h1>Tecnico</h1>
    <p>Home. / Tecnico</p>
</div>
<!-- end top bar -->
<!-- main container -->
<div class="main-container portfolio-inner clearfix">
    <!-- portfolio div -->
    <div class="portfolio-div">
        <div class="portfolio">
            <!-- portfolio_filter -->
            <div class="categories-grid wow fadeInLeft">
                <nav class="categories text-center">
                    <ul>
                        <li>
                            @Html.ActionLink("Lista", "Index")
                        </li>
                        <li>@Html.ActionLink("Registrar", "Create")</li>
                        <li><a href="" class="active">Detalle</a></li>
                        <li>@Html.ActionLink("Editar", "Edit", new { id = Model.id_tecnico })</li>
                    </ul>
                </nav>
            </div>
            <!-- portfolio_filter -->
            <!-- portfolio_container -->
            <div class="container">
                <!-- single work -->
                <div class="col-sm-4-1 offset-md-1-1 py-4 border-op  fashion logo">


                    <div>

                        <hr />
                        <dl class="dl-horizontal">
                            <dt>
                                @Html.LabelFor(model => model.id_tecnico, "Identificacion")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.id_tecnico)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.tec_nom, "Nombre")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.tec_nom)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.tec_tel, "Telefono")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.tec_tel)
                            </dd>

                        </dl>
                    </div>
                    <p>
                        |
                    </p>

                </div>
                <!-- end single work -->


            </div>
            <!-- end portfolio_container -->
        </div>
        <!-- portfolio -->
    </div>
    <!-- end portfolio div -->
</div>
<!-- end main container -->
