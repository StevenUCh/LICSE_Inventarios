@model IEnumerable<LICSE_Inventarios.Models.SOLICITUD>
@{
    ViewBag.Title = "Solicitudes";
}

<!-- top bar -->
<div class="top-bar solicitudes">
    <h1>Solicitudes</h1>
    <p>Home. / Solicitudes</p>
</div>
<!-- end top bar -->
<!-- main container -->
<div class="main-container portfolio-inner clearfix">
    <!-- portfolio div -->
    <div class="portfolio-div">
        <div class="portfolio">
            <!-- portfolio_filter -->
            <div class="categories-grid wow fadeInLeft">
                <nav class="categories text-center">
                    <ul>
                        <li><a href="" class="active">Lista</a></li>
                        <li>@Html.ActionLink("Registrar", "Create")</li>
                        <li>@Html.ActionLink("Registrar tecnico", "Create", "TECNICOS")</li>

                    </ul>
                </nav>
            </div>
            <!-- portfolio_filter -->
            <!-- portfolio_container -->
            <div class="container">
                <!-- single work -->
                <div class="row">
                    <div class="col-sm-4-1 offset-md-1-1 py-4 border-op  fashion logo">

                        <table class="table">
                            <tr>
                                <th>
                                    <p>Identificacion</p>
                                </th>
                                <th>
                                    <p>Fecha Registro</p>
                                </th>
                                <th>
                                    <p>Fecha Instalacion</p>
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.solicitante)
                                </th>
                                <th>
                                    <p>Sede</p>
                                </th>
                                <th>
                                    <p>Tecnico</p>
                                </th>
                                <th>
                                    <p>Usuario</p>
                                </th>
                                <th></th>
                            </tr>

                            @foreach (var item in Model)
                            {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.id_solicitud)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.sol_fecha)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.fecha_progra)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.solicitante)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SEDE1.sede_nombre)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TECNICO1.tec_nom)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.USUARIO1.usu_nombre)
                        </td>
                        <td>
                            @Html.ActionLink("Editar", "Edit", new { id = item.id_solicitud }) |
                            @Html.ActionLink("Detalles", "Details", new { id = item.id_solicitud }) |
                            @Html.ActionLink("Eliminar", "Delete", new { id = item.id_solicitud })
                        </td>
                    </tr>}

                        </table>
                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                @Html.ActionLink("Volver", "Inicio", "", htmlAttributes: new { @class = "btn btn-default" })
                            </div>
                        </div>


                    </div>
                    <!-- end single work -->
                </div>

            </div>
            <!-- end portfolio_container -->
        </div>
        <!-- portfolio -->
    </div>
    <!-- end portfolio div -->
</div>
<!-- end main container -->
